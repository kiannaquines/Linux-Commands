sudo tshark -i <INTERFACE_NAME> -a duration:<DURATION_MS> -Y '(wlan.fc.type_subtype == 0x00 or wlan.fc.type_subtype == 0x02 or wlan.fc.subtype == 0x04) or (wlan.fc.type == 2) and radiotap.dbm_antsignal >= -65' -e frame.time -e wlan.sa -e wlan.da -e radiotap.dbm_antsignal -e wlan.fc.type -e wlan.fc.subtype -e wlan.fc.type_subtype -V -T json

sudo tshark -i wlan1 -l -Y "wlan.fc.type == 0 && wlan.fc.subtype == 0x04" -T fields -e wlan.ta -e wlan.sa -e wlan.seq -e frame.time_epoch -e radiotap.dbm_antsignal -e wlan.ssid -e wlan_radio.frequency -e wlan.fc.subtype -e wlan.tag.number -e wlan.tag.vendor.oui.type -e wlan.fcs -e frame.number -E header=y -E separator=, -E quote=d -E occurrence=f > results_probe.txt
