sudo tshark -i <INTERFACE_NAME> -a duration:<DURATION_MS> -Y '(wlan.fc.type_subtype == 0x00 or wlan.fc.type_subtype == 0x02 or wlan.fc.subtype == 0x04) or (wlan.fc.type == 2) and radiotap.dbm_antsignal >= -65' -e frame.time -e wlan.sa -e wlan.da -e radiotap.dbm_antsignal -e wlan.fc.type -e wlan.fc.subtype -e wlan.fc.type_subtype -V -T json

sudo tshark -i wlan1 -l -Y "wlan.fc.type == 0 && wlan.fc.subtype == 0x04" -T fields -e wlan.ta -e wlan.sa -e wlan.seq -e frame.time_epoch -e radiotap.dbm_antsignal -e wlan.ssid -e wlan_radio.frequency -e wlan.fc.subtype -e wlan.tag.number -e wlan.tag.vendor.oui.type -e wlan.fcs -e frame.number -E header=y -E separator=, -E quote=d -E occurrence=f > results_probe.txt

sudo tshark -i wlan1 -l -Y "wlan.fc.type == 0 && wlan.fc.subtype == 0x04 && wlan_radio.frequency <= 2484" -T fields -e wlan.ta -e frame.encap_type -e frame.time -e frame.len -e frame.cap_len -e frame.marked -e frame.ignored -e radiotap.present.word -e radiotap.present.tsft -e radiotap.present.flags -e radiotap.present.rate -e radiotap.present.channel -e radiotap.present.fhss -e radiotap.present.dbm_antsignal -e radiotap.present.dbm_antnoise -e radiotap.present.lock_quality -e radiotap.present.tx_attenuation -e radiotap.present.db_tx_attenuation -e radiotap.present.dbm_tx_power -e radiotap.present.antenna -e radiotap.present.db_antsignal -e radiotap.present.db_antnoise -e radiotap.present.rxflags -e radiotap.present.txflags -e radiotap.present.data_retries -e radiotap.present.xchannel -e radiotap.present.mcs -e radiotap.present.ampdu -e  radiotap.present.vht -e radiotap.present.timestamp -e radiotap.present.he -e radiotap.present.he_mu -e radiotap.present.0_length.psdu -e radiotap.present.l_sig -e radiotap.present.tlv -e radiotap.present.rtap_ns  -e radiotap.present.vendor_ns -e radiotap.present.ext -E header=y -E separator=, -E quote=d -E occurrence=f > final_dataset_features.txt

sudo tshark -i wlan1 -l -Y "wlan.fc.type == 0 && wlan.fc.subtype == 0x04 && wlan_radio.frequency <= 2484 && radiotap.dbm_antsignal >= -30" -T fields -e wlan.ta -e frame.encap_type -e frame.time -e frame.len -e frame.cap_len -e frame.marked -e frame.ignored -e radiotap.present.word -e radiotap.present.tsft -e radiotap.present.flags -e radiotap.present.rate -e radiotap.present.channel -e radiotap.present.fhss -e radiotap.present.dbm_antsignal -e radiotap.present.dbm_antnoise -e radiotap.present.lock_quality -e radiotap.present.tx_attenuation -e radiotap.present.db_tx_attenuation -e radiotap.present.dbm_tx_power -e radiotap.present.antenna -e radiotap.present.db_antsignal -e radiotap.present.db_antnoise -e radiotap.present.rxflags -e radiotap.present.txflags -e radiotap.present.data_retries -e radiotap.present.xchannel -e radiotap.present.mcs -e radiotap.present.ampdu -e  radiotap.present.vht -e radiotap.present.timestamp -e radiotap.present.he -e radiotap.present.he_mu -e radiotap.present.0_length.psdu -e wlan.tag.number -e wlan.tag.oui -e wlan.tag.vendor.oui.type -e wlan_radio.data_rate -e wlan_radio.phy -e wlan_radio.preamble -e wlan_radio.signal_dbm -e radiotap.present.l_sig -e radiotap.present.tlv -e radiotap.present.rtap_ns  -e radiotap.present.vendor_ns -e radiotap.present.ext -E header=y -E separator=, -E quote=d -E occurrence=f > final_dataset_features_test_2.txt
